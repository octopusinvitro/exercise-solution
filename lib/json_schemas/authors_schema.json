{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Author list",
  "description": "A list of authors with articles",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "articles": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": [
      "name",
      "articles"
    ]
  }
}
